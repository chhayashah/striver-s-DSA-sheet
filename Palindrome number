import java.util.Scanner;
public class Solution {
    public static boolean palindromeNumber(int n){
        if(n<0){
            return false;
        }
        // Write your code here.
        int count=0;
        while(n>0){
            int last=n%10;
            count=count*10+last;
            n=n/10;
        }
        return true;
    }

     


}
